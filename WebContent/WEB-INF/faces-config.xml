<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	
	<managed-bean>
		
		<description> açıklama vs</description>
		<managed-bean-name>yonetimliBean</managed-bean-name>
		<managed-bean-class>com.cplbilisim.pojo.YonetimliBean</managed-bean-class>
		
		<managed-bean-scope>request</managed-bean-scope>
	
	
	</managed-bean>

	<managed-bean>
		<description> kapsam aralığı session dır. bu tipteki
		nesneler sayfalar arasında kolayca kullanılarak yönetilirler
		</description>
		<managed-bean-name>yonetimliBean2</managed-bean-name>
		<managed-bean-class>com.cplbilisim.pojo.YonetimliBean2</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	
	
	
	</managed-bean>
	
	<managed-bean>
		
		<description> açıklama vs</description>
		<managed-bean-name>yonetimliBean3</managed-bean-name>
		<managed-bean-class>com.cplbilisim.pojo.YonetimliBean3</managed-bean-class>
		
		<managed-bean-scope>request</managed-bean-scope>
	
	
	</managed-bean>
	
	<navigation-rule>
	<from-view-id>/index.jsp</from-view-id>
	
	<navigation-case>
	<from-outcome>sayfa2</from-outcome>
	<to-view-id>/sayfa2.jsf</to-view-id>
	
	</navigation-case>
	
	
	
	</navigation-rule>
	
	

</faces-config>
